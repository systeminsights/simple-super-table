{
  "name": "simple-super-table",
  "version": "2.0.0",
  "description": "React component for rendering low volume data tables (~500). Provides search, sort and csv export for free.",
  "keywords": [
    "react",
    "react-component"
  ],
  "repository": "https://github.com/santhosbaala/simple-super-table",
  "license": "MIT",
  "author": "System Insights Inc.",
  "main": "./dist/simple-super-table.min.js",
  "scripts": {
    "build": "webpack --watch",
    "build-dist": "NODE_ENV=development npm test && NODE_ENV=production webpack --release",
    "serve": "browser-sync start --server --files \"index.html,dist/**\"",
    "start": "NODE_ENV=development parallelshell \"npm run build\" \"npm run serve\"",
    "test": "NODE_ENV=development ./node_modules/.bin/mocha test"
  },
  "dependencies": {
    "d3-scale": "^0.1.1",
    "ramda": "^0.15.1",
    "react": "^0.13.3",
    "to-csv": "^0.1.0"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.1.11",
    "autoprefixer-loader": "^1.2.0",
    "babel": "^5.6.14",
    "babel-core": "^5.2.13",
    "babel-eslint": "^3.0.1",
    "babel-loader": "^5.0.0",
    "browser-sync": "^2.7.1",
    "chai": "^3.0.0",
    "css-loader": "^0.12.0",
    "del": "^1.1.1",
    "eslint": "^0.20.0",
    "eslint-loader": "^0.11.1",
    "eslint-plugin-react": "^2.2.0",
    "jquery": "^2.1.4",
    "jsdom": "^5.6.0",
    "json-loader": "^0.5.2",
    "less": "^2.5.0",
    "less-loader": "^2.2.0",
    "minimist": "^1.1.1",
    "mocha": "^2.2.5",
    "parallelshell": "^1.1.1",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.12.1",
    "url-loader": "^0.5.5",
    "webpack": "^1.8.11"
  },
  "engines": {
    "node": ">= 0.10",
    "npm": ">= 2.1"
  }
}
